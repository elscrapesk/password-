print("welcome to the system")
print("enter username")
usrname=input("")
print("enter password")
usrpassword=input("")
if usrname==("USERNAME1") and usrpassword==("PASSWORD1"):
    print("welcome to the system")
    break
elif usrname=="(special_username") and usrpassword==("special_password"):
    print("welcome to the special system")
    break
else:
    print("not found")
